                  <Button fx:id="charSixButton" mnemonicParsing="false" text="Character 6" AnchorPane.bottomAnchor="10.0" AnchorPane.leftAnchor="10.0" AnchorPane.rightAnchor="10.0" AnchorPane.topAnchor="10.0" />
